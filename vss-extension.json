{
	"manifestVersion": 1,
	"id": "tag-git-release",
	"name": "Tag Git on Release",
	"description": "Create an annotated git tag in the release pipeline",
	"version": "1.1.0",
	"publisher": "pwshliquori",
	"public":  true,
	"baseUri": "https://github.com/USF-HII",
	"scopes": [],
	"tags": [
		"git",
		"tags",
		"release"
	],
	"links": {
		"home": {
			"uri": "https://github.com/USF-HII/azuredevops-extension-tag-git-release"
		},
		"learn": {
			"uri": "https://github.com/USF-HII/azuredevops-extension-tag-git-release"
		},
		"support": {
			"uri": "https://github.com/USF-HII/azuredevops-extension-tag-git-release/issues"
		},
		"repository": {
			"uri": "https://github.com/USF-HII/azuredevops-extension-tag-git-release"
		}
	},
	"repository": {
		"type": "git",
		"uri": "https://github.com/USF-HII/azuredevops-extension-tag-git-release"
	},
	"branding": {
		"color": "rgb(0, 114, 186)",
		"theme": "dark"
	},
	"targets": [
		{
			"id": "Microsoft.VisualStudio.Services"
		}
	],
	"categories": [
		"Azure Pipelines"
	],
	"icons": {
		"default": "logo.png"
	},
	"files": [
		{ "path": "Task" }
	],
	"content": {
		"details": {
			"path": "overview.md"
		},
		"license": {
			"path": "license.md"
		}
	},
	"contributions": [
		{
			"id": "tag-git-release",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "Task"
			}
		}
	]
}